{
  "extends": ["tslint:latest", "tslint-config-prettier", "tslint-plugin-prettier", "tslint-react"],
  "rules": {
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "prettier": {
      "options": {
          "printWidth": 120,
          "tabWidth": 4,
          "trailingComma": "all"
      }
    },
    "jsx-no-lambda": false,
    "no-submodule-imports": [
      true,
      "history"
    ],
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "no-reference": false,
    "no-implicit-dependencies": false,
    "jsx-no-multiline-js": false,
    "no-console": [
      true,
      "log"
    ]
  }
}
